{
  "name": "phone-numbers-db-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "NODE_ENV=development webpack",
    "dev": "node ./build/index.js",
    "prestart": "NODE_ENV=production webpack",
    "start": "npm run codegen && node ./build/index.js",
    "db:run": "docker compose up -d --build mysql",
    "sequelize:compile": "./scripts/compile-sequelize-files.sh",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.0.1",
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "graphql": "^16.6.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "sequelize": "6.28.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/typescript": "2.7.5",
    "@graphql-codegen/typescript-resolvers": "2.7.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "1.9.3",
    "@types/node": "18.7.13",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-webpack-plugin": "3.2.0",
    "prettier": "2.7.1",
    "sequelize-cli": "6.5.2",
    "ts-loader": "9.3.1",
    "typescript": "4.8.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  }
}
